{
    "name": "thecodingmachine/service-provider-bridge-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony Bundle allows Symfony applications to use service providers as defined in container-interop/service-provider",
    "keywords": ["PSR-11", "Symfony", "container-interop", "service-provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "container-interop/service-provider": "~0.4.0",
        "thecodingmachine/service-provider-registry": "^3.0",
        "thecodingmachine/common-factories": "^0.4",
        "thecodingmachine/discovery": "^1.2",
        "container-interop/container-interop": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": { "TheCodingMachine\\Interop\\ServiceProviderBridgeBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "TheCodingMachine\\Interop\\ServiceProviderBridgeBundle\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
